title: Introstories - French
-->
<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">
  <title>French Introstories - Translamore Blog</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <link rel="apple-touch-icon" sizes="180x180" href="/images/favicon/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon/favicon-16x16.png">
  <link rel="manifest" href="/images/favicon/site.webmanifest">
  <link rel="mask-icon" href="/images/favicon/safari-pinned-tab.svg" color="#5bbad5">
  <link rel="shortcut icon" href="/images/favicon/favicon.ico">

  <link href="/styles/styles.css" rel="stylesheet">
  <link href="/styles/home.css" rel="stylesheet">
  <link href="/styles/post.css" rel="stylesheet">
</head>

<body>
  {{ components.nav() }}

  <header>
    <h1>French</h1>
    <p class="tagline">Introstories in Fran√ßais</p>
  </header>

  <main>
    <section>
      <ul class="post-list">
      {% for page in pages.byTag("introstories", sortBy="publishDate=desc") %}
        {% if page.data['language'] == 'fr' and page.data['publishDate'] %}
        <li>
          <div class="date">{{ page.data['publishDate'] }}</div>
          <a class="title" href="{{  page.data['url']  }}">{{  page.data['title']  }}</a>
          <p class="description">{{  page.data['description']  }}</p>
        </li>
        {% endif %}
      {% endfor %}
      </ul>
    </section>
  </main>

  {{  components.footer()  }}
</body>
</html>
